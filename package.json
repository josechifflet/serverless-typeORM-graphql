{
  "name": "express-typescript-graphql",
  "version": "0.0.1",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "start": "serverless offline",
    "lint": "eslint src/**/**/*.ts && eslint tests/**/**/*.ts",
    "tsc": "tsc",
    "test": "NODE_ENV=test jest --runInBand",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen",
    "generate": "graphql-codegen --config codegen.yml",
    "codecov": "codecov",
    "migrate:generate": "sequelize-cli migration:generate --name",
    "migrate:dev": "cross-env NODE_ENV=development sequelize db:migrate",
    "migrate:prod": "cross-env NODE_ENV=production sequelize db:migrate",
    "migrate": "yarn migrate:dev && yarn migrate:prod"
  },
  "dependencies": {
    "apollo-server-lambda": "^2.19.1",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "firebase": "^8.2.0",
    "firebase-admin": "^9.4.2",
    "graphql": "^15.4.0",
    "graphql-import": "^1.0.2",
    "graphql-middleware": "^6.0.0",
    "graphql-subscriptions": "^1.1.0",
    "helmet": "^4.3.1",
    "http": "0.0.1-security",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.25.0",
    "mysql2": "^2.2.5",
    "pg": "^8.5.1",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.29",
    "typeorm-encrypted": "^0.5.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/introspection": "^1.11.2",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.1",
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/bluebird": "^3.5.33",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.6",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.16",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "codecov": "^3.8.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "^6.8.0",
    "serverless-webpack": "^5.3.5",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
